  "scripts": {
    "lint": "eslint . --ext .ts --fix-dry-run",
    "test": "jest",
    "gen-openaapi": "tsoa spec",
    "build": "tsc && tsoa spec && cp .env dist/src",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "prod": "knex migrate:latest --knexfile dist/src/external/db/knexfile.js --debug && pm2-runtime start pm2.json",
    "migrate_make": "knex --knexfile src/external/db/knexfile.js --debug migrate:make",
    "migrate_up": "knex migrate:up --knexfile src/external/db/knexfile.ts --debug",
    "migrate_down": "knex migrate:down --knexfile src/external/db/knexfile.ts --debug",
    "migrate_rollback": "knex migrate:rollback --knexfile src/external/db/knexfile.ts --debug",
    "migrate_latest": "knex migrate:latest --knexfile src/external/db/knexfile.ts --debug",
    "run_seed": "knex seed:run --knexfile src/external/db/knexfile.ts --debug"
  },
  "devDependencies": {
